SET Citation = {"PubMed", "23356410"}

SET Evidence = "Moreover,
HDAC6 was able to deacetylate other substrates
and to bind ubiquitin, thus modulating cell protective
response to cytotoxic accumulation of misfolded and
aggregated proteins [19,29,30]."
p(HGNC:HDAC6) -> p(MESH:Proteins,pmod(CONSO:misfolded),pmod(Ub))
p(HGNC:HDAC6) -> p(MEHS:Proteins,pmod(CONSO:"protein aggregates"),pmod(Ub))
p(MESH:Proteins,pmod(CONSO:misfolded)) -> path(MESH:"Cytotoxicity, Immunologic")
p(MEHS:Proteins,pmod(CONSO:"protein aggregates")) -> path(MESH:"Cytotoxicity, Immunologic")
p(HGNC:HDAC6) reg path(MESH:Cytoprotection)

SET Evidence = "As a matter of fact, HDAC pan inhibitors
were shown to significantly improve long term
memory and learning after inducing neuronal loss
in mice."
SET Species = "10090"
a(MESH:"Histone Deacetylase Inhibitors") pos bp(GO:"long-term memory")
a(MESH:"Histone Deacetylase Inhibitors") pos bp(GO:learning)
a(MESH:"Histone Deacetylase Inhibitors") -> bp(GO:"neuron death")
UNSET Species

SET Evidence = "These findings could be correlated with the increase
of the acetylation of histones H3 and H4 in the
hippocampus and the cortex of mice few hours
after being treated [47,48]"
#these finding - increase memory and learning
SET Species = "10090"
SET MeSHAnatomy = {"Hippocampus","Neocortex"}
a(MESH:"Histone Deacetylase Inhibitors") -> bp(GO:"histone H3 acetylation")
a(MESH:"Histone Deacetylase Inhibitors") -> bp(GO:"histone H4 acetylation")
bp(GO:"histone H3 acetylation") -- bp(GO:"long-term memory")
bp(GO:"histone H4 acetylation") -- bp(GO:"long-term memory")
bp(GO:"histone H3 acetylation") -- bp(GO:learning)
bp(GO:"histone H4 acetylation") -- bp(GO:learning)
UNSET MeSHAnatomy
UNSET Species

SET Evidence = "The use of
the HDAC inhibitor vorinostat (SAHA) was shown to
promote the expression of these genes by increasing the
acetylation of H4K12, thus resulting in improved cognitive
function in mice [49]."
#in hippocampus
SET Species = "10090"
SET MeSHAnatomy = "Hippocampus"
a(CHEBI:vorinostat) -| p(MESH:"Histone Deacetylases")
a(CHEBI:vorinostat) -> bp(GO:"histone H4-K12 acetylation")
bp(GO:"histone H4-K12 acetylation") -> bp(GO:cognition)
UNSET MeSHAnatomy
UNSET Species

#in CK-p25 mice
SET Species = "10090"
SET MeSHDisease = "Alzheimer Disease"

SET Evidence = "These animals are used in models of NDs,
since they overexpress protein p25, which has been
associated with some AD features : it accumulates in
neurons of AD patients and complexes with Cdk5 kinase."
#p25 - CDK5 fragment formed
p(HGNC:CDK5,frag("?"))
p(HGNC:CDK5,frag("?"))
